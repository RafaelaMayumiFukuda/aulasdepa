Algoritmo "a"

Var

  d1:real
  d2:real
  area:real

Inicio
    escreval("Digite o valor da diagonal maior do losango:")
    leia(d1)
    escreval("Digite o valor da giagonal menor do losango:")
    leia(d2)
    
    area <- (d1 * d2)/2
    
    escreval("A área do losango é: ",area)



Fimalgoritmo