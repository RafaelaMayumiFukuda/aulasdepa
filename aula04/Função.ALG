Algoritmo "fUNÇÃO"
//y = 7x + 3x - 8 * 6
Var

 x,y:inteiro
Inicio

    escreval("Esse programa vai calcular o y da função: ")
    escreval("f(x)= 7x + 3x - 8 * 6 ")
    escreval("Digite o valor de x: ")
    leia(x)
    
     y <- (7 * x ) + (3 * x) - (8 * 6)
     
     escreval("O valor de y é ",y)

Fimalgoritmo